[core]
    autocrlf     = input
    safecrlf     = warn
    editor       = nano
[color]
    ui           = true
[color "branch"]
    current      = yellow reverse
    local        = yellow
    remote       = green
[color "diff"]
    meta         = yellow bold
    frag         = magenta bold
    old          = red bold
    new          = green bold
[push]
    default = current
[rebase]
    autosquash = true
[http]
    postBuffer = 1048576000
[commit]
    gpgsign = true
